<script>
  document.addEventListener("DOMContentLoaded", function () {
    document.querySelectorAll(".lazy-bg").forEach(el => {
      const observer = new IntersectionObserver(entries => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            const bg = entry.target.getAttribute("data-bg");
            if (bg) {
              entry.target.style.backgroundImage = `url('${bg}')`;
              entry.target.removeAttribute("data-bg");
              observer.unobserve(entry.target);
            }
          }
        });
      }, {
        rootMargin: '0px',
        threshold: 0.1
      });
      observer.observe(el);
    });
  });
</script>

	<script>
		const savedMode = localStorage.getItem("mode");

		// Apply the saved mode as soon as possible
		if (savedMode === "dark") {
			document.body.classList.add("dark");
		} else {
			document.body.classList.add("light");
		}

		window.addEventListener("DOMContentLoaded", () => {
			const checkbox = document.getElementById("checkbox");

			// Ensure the checkbox reflects the saved mode
			if (savedMode === "dark") {
				checkbox.checked = true;
			} else {
				checkbox.checked = false;
			}

			// Add event listener to toggle modes and save the preference
			checkbox.addEventListener("change", () => {
				if (checkbox.checked) {
					document.body.classList.add("dark");
					document.body.classList.remove("light");
					localStorage.setItem("mode", "dark");
				} else {
					document.body.classList.add("light");
					document.body.classList.remove("dark");
					localStorage.setItem("mode", "light");
				}
			});
		});
	</script>
	<script>
	   const adLink = "https://spendharassingwarmly.com/rwn3p21x?key=6f0ffbd9124b516cb6fa9c3e72479efa";
const intervals = [240000, 300000, 600000, 1200000, 300000, 300000]; // 2m, 5m, 10m, 20m, 5m, 5m
let intervalIndex = 0;

function showFredflixModal() {
    const overlay = document.getElementById("fredflix-modal-overlay");
    overlay.style.display = "flex";
}

function hideFredflixModalAndOpenAd() {
    window.open(adLink, "_blank");
    document.getElementById("fredflix-modal-overlay").style.display = "none";
    scheduleNextPopup();
}

function scheduleNextPopup() {
    if (intervalIndex >= intervals.length) intervalIndex = intervals.length - 1;
    setTimeout(showFredflixModal, intervals[intervalIndex++]);
}

document.addEventListener("DOMContentLoaded", function () {
    setTimeout(showFredflixModal, 4000); // First after 4 sec

    document.body.insertAdjacentHTML("beforeend", `
    <div id="fredflix-modal-overlay">
        <div id="fredflix-modal">
            <h2>Support Our Service</h2>
            <p>We keep this service free by showing occasional ads. Your support helps us continue.</p>
            <iframe src="${adLink}" loading="lazy"></iframe>
            <button id="fredflix-continue-btn">Continue</button>
        </div>
    </div>
    `);

    document.getElementById("fredflix-continue-btn")
        .addEventListener("click", hideFredflixModalAndOpenAd);
});
 
	</script>
</body>
