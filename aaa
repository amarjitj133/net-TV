	<script>
	// Preloader script
	window.addEventListener("load", function() {
		const loader = document.getElementById("loader");
		const metaBanner = document.querySelector(".meta-banner");
		const chatToggle = document.getElementById("chat-toggle");
		const header = document.querySelector("header");
		const main = document.querySelector("main");
		const footer = document.querySelector("footer");

		if (loader) {
			loader.style.display = "none";
		}

		// Show banner img after preloader is hidden
		if (metaBanner) {
			metaBanner.style.display = "flex"; // or "block"
		}

		// Show chat icon after preloader is hidden
		if (chatToggle) {
			chatToggle.style.display = "flex"; // or "block"
		}

		// Show header, main, and footer after loading
		if (header) {
			header.style.display = "block";
		}
		if (main) {
			main.style.display = "block";
		}
		if (footer) {
			footer.style.display = "block";
		}
	});
	</script>
