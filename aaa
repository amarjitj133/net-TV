	<div id="search-results"></div>

<script>
var searchInput = document.getElementById('searchInput');
var searchResults = document.getElementById('search-results');
var postTypeSelect = document.getElementById('location'); // Dropdown for post type

// Function to fetch results and handle empty search terms
function fetchResults() {
    var searchTerm = searchInput.value.trim(); // Ensure the input is trimmed
    var postType = postTypeSelect.value; // Get selected post type

    if (searchTerm === '') {
        searchResults.innerHTML = ''; // Clear previous results
        searchResults.style.display = 'none'; // Hide the results container
        return;
    }

    searchResults.style.display = 'block'; // Show results container

    // Simulate an AJAX call for fetching results
    var xhr = new XMLHttpRequest();
    xhr.open('POST', 'https://live.fredflix.fun/wp-admin/admin-ajax.php');
    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhr.onreadystatechange = function() {
        if (xhr.readyState === 4 && xhr.status === 200) {
            searchResults.innerHTML = xhr.responseText; // Populate results
        }
    };
    xhr.send('action=live_search&searchTerm=' + encodeURIComponent(searchTerm) + '&postType=' + encodeURIComponent(postType));
}

// Event listener for input keyup
searchInput.addEventListener('keyup', function() {
    fetchResults();
});

// Event listener to hide results when clicking outside
document.addEventListener('click', function(event) {
    if (!searchInput.contains(event.target) && !searchResults.contains(event.target)) {
        searchResults.style.display = 'none'; // Hide results container
    }
});
</script>
